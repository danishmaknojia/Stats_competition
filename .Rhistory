#"Date:", date,
"\n") # Progress
result <- tryCatch({
cbbdata::cbd_torvik_game_stats(year = 2025, team = team, opp = opp,
#date = date
)
}, error = function(e) {
cat("Error fetching data for Team:", team, "Opp:", opp,  "\n")
NULL
})
# Append the result to the results list
results_list[[length(results_list) + 1]] <- result
}
}
# Combine all results into a single data frame, if possible
final_data <- do.call(rbind, results_list)
